{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s -n \"build:*\"",
    "build:css": "yarn tailwindcss -o ./app/styles/tailwind.css",
    "build:remix": "yarn remix build",
    "dev": "cross-env NODE_ENV=development run-p \"dev:*\"",
    "dev:css": "yarn build:css --watch",
    "dev:remix": "yarn remix dev",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "deploy": "caprover deploy -d"
  },
  "dependencies": {
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@types/node-mailjet": "^3.3.9",
    "@urql/core": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "node-mailjet": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "eslint": "^8.32.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@3.2.0"
}